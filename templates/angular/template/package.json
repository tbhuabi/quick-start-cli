{
  "name": "test",
  "version": "1.0.0",
  "description": "A webpack starter for Angular",
  "main": "server/server.js",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development node server/server.js",
    "doc": "cross-env NODE_ENV=documentation node server/doc-server.js",
    "pack": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config config/webpack.prod.js --progress --profile --bail --optimize-minimize",
    "production": "npm run build && node server/product-server.js"
  },
  "keywords": [
    "angular2",
    "ng2",
    "typescript",
    "ts",
    "webpack",
    "es6"
  ],
