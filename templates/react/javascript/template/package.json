{
  "name": "React-starter",
  "version": "1.0.0",
  "description": "A webpack starter for React",
  "main": "server/server.js",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development node server/server.js",
    "pack": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail --optimize-minimize",
    "production": "npm run build && node server/product-server.js"
  },
  "keywords": [
    "react",
    "webpack",
    "es6"
  ],
  "author": "",
  "dependencies": {
    "normalize.css": "8.0.0",
    "prop-types": "15.6.1",
    "react": "16.3.2",
    "react-css-modules": "4.3.0",
    "react-dom": "16.3.2",
    "react-router-dom": "4.2.2"
  },
  "devDependencies": {
    "autoprefixer": "8.4.1",
    "babel": "6.23.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "connect-history-api-fallback": "1.5.0",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "eventsource-polyfill": "0.9.6",
    "express": "4.16.3",
    "express-favicon": "2.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "http-proxy-middleware": "0.18.0",
    "ip": "1.1.5",
    "jasmine-core": "3.1.0",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "3.0.0",
    "open": "0.0.5",
    "postcss": "6.0.2",
    "postcss-loader": "2.0.6",
    "react-addons-test-utils": "15.6.0",
    "rimraf": "2.6.1",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "webpack": "3.8.1",
    "webpack-dev-middleware": "1.12.1",
    "webpack-dev-server": "2.9.4",
    "webpack-hot-middleware": "2.21.0",
    "webpack-merge": "4.1.1"
  }
}
