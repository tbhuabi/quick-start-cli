    "http-proxy-middleware": "0.18.0",
    "ip": "1.1.5",
    "jasmine-core": "3.1.0",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "3.0.0",
