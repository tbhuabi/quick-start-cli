{
  "name": "React-starter",
  "version": "1.0.0",
  "description": "A webpack starter for React",
  "main": "server/server.js",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development node server/server.js",
    "pack": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail --optimize-minimize",
    "production": "npm run build && node server/product-server.js"
  },
  "keywords": [
    "react",
    "webpack",
    "es6"
  ],
  "author": "",
  "dependencies": {
    "normalize.css": "8.0.1",
    "react": "16.8.6",
    "react-css-modules": "4.7.9",
    "react-dom": "16.8.6",
    "react-router-dom": "5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@hot-loader/react-dom": "^16.8.6",
    "@types/jasmine": "3.3.12",
    "@types/node": "12.0.0",
    "@types/react": "16.8.16",
    "@types/react-addons-test-utils": "0.14.24",
    "@types/react-css-modules": "4.6.2",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "4.3.3",
    "autoprefixer": "9.5.1",
    "babel-loader": "^8.0.5",
    "connect-history-api-fallback": "1.6.0",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "eventsource-polyfill": "0.9.6",
    "express": "4.16.4",
    "express-favicon": "2.0.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "3.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "http-proxy-middleware": "0.19.1",
    "ip": "1.1.5",
    "jasmine-core": "3.4.0",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
