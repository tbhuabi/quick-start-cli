{
  "name": "vue",
  "version": "1.0.0",
  "description": "A webpack starter for Vue2",
  "main": "server/server.js",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development node server/server.js",
    "pack": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail --optimize-minimize",
    "production": "npm run build && node server/product-server.js"
  },
  "keywords": [
    "vue2",
    "webpack",
    "es6"
  ],
  "author": "tanbo",
  "license": "ISC",
  "dependencies": {
    "normalize.css": "7.0.0",
    "vue": "2.3.4",
    "vue-router": "2.5.3"
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.24.1",
    "connect-history-api-fallback": "1.3.0",
    "cross-env": "5.0.1",
    "css-loader": "0.28.4",
    "eslint": "3.19.0",
    "eslint-config-standard": "10.2.1",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-loader": "1.8.0",
    "eslint-plugin-html": "3.0.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-node": "5.0.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "eventsource-polyfill": "0.9.6",
    "express": "4.15.3",
    "express-favicon": "2.0.0",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.28.0",
    "http-proxy-middleware": "0.17.4",
    "ip": "1.1.5",
    "jasmine-core": "2.6.3",
    "karma": "1.7.0",
    "karma-jasmine": "1.1.0",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-phantomjs-shim": "1.4.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.3",
